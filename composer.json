{
    "name": "symfony/symfony-demo",
    "license": "MIT",
    "type": "project",
    "description": "Symfony Demo Application",
    "require": {
        "php": "^7.1.3",
        "ext-pdo_sqlite": "*",
        "doctrine/doctrine-bundle": "^1.9.1",
        "doctrine/doctrine-fixtures-bundle": "^3.0.2",
        "doctrine/doctrine-migrations-bundle": "^v1.3.1",
        "doctrine/orm": "^v2.6.1",
        "erusev/parsedown": "^1.7.1",
        "ezyang/htmlpurifier": "^v4.10.0",
        "giggsey/libphonenumber-for-php": "^8.9.7",
        "messagebird/php-rest-api": "^v1.9.4",
        "ramsey/uuid": "^3.7.3",
        "ramsey/uuid-doctrine": "^1.4.3",
        "sensio/framework-extra-bundle": "^v5.1.6",
        "sensiolabs/security-checker": "^v4.1.8",
        "symfony/asset": "^v4.1.0",
        "symfony/expression-language": "^v4.1.0",
        "symfony/flex": "^v1.2.6",
        "symfony/form": "^v4.1.0",
        "symfony/framework-bundle": "^v4.1.0",
        "symfony/lock": "^v4.1.0",
        "symfony/monolog-bundle": "^v3.2.0",
        "symfony/polyfill-apcu": "^v1.8.0",
        "symfony/psr-http-message-bridge": "^v1.0.2",
        "symfony/security-bundle": "^v4.1.0",
        "symfony/swiftmailer-bundle": "^v3.2.2",
        "symfony/templating": "^v4.1.0",
        "symfony/translation": "^v4.1.0",
        "symfony/twig-bundle": "^v4.1.0",
        "symfony/validator": "^v4.1.0",
        "symfony/web-server-bundle": "^v4.1.0",
        "symfony/yaml": "^v4.1.0",
        "twig/extensions": "^v1.5.1",
        "twilio/sdk": "^5.19.2",
        "white-october/pagerfanta-bundle": "^v1.2.0",
        "zendframework/zend-diactoros": "^1.7.2"
    },
    "require-dev": {
        "codeception/codeception": "^2.4.2",
        "dama/doctrine-test-bundle": "^v5.0.1",
        "friendsofphp/php-cs-fixer": "^v2.11.1",
        "hgraca/doctrine-test-db-regeneration-bundle": "^v1.1.0",
        "malukenho/mcbumpface": "^0.1.3",
        "mockery/mockery": "^1.1.0",
        "phpunit/phpunit": "^7.1.5",
        "roave/security-advisories": "dev-master",
        "symfony/browser-kit": "^v4.1.0",
        "symfony/css-selector": "^v4.1.0",
        "symfony/debug-bundle": "^v4.1.0",
        "symfony/dotenv": "^v4.1.0",
        "symfony/phpunit-bridge": "^v4.1.0",
        "symfony/stopwatch": "^v4.1.0",
        "symfony/web-profiler-bundle": "^v4.1.0"
    },
    "config": {
        "platform": {
            "php": "7.1.3"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Acme\\App\\": "src/",
            "Acme\\PhpExtension\\": "lib/php-extension/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Acme\\App\\Build\\": "build/",
            "Acme\\App\\Test\\": "tests/",
            "Acme\\PhpExtension\\Test\\": "lib/php-extension/tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01BY8QJHBGTYRP06WQFNY99EKP",
            "allow-contrib": true
        }
    }
}
